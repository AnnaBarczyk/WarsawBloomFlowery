                                    <img class="img-responsive img-centered" src="~/lib/bootstrap/dist/img/logo1.png" alt="">
                                </a>
                            </li>
                            <li>
                                <a class="page-scroll" href="#services">Services</a>
                            </li>
                            <li>
                                <a class="page-scroll" href="#portfolio">Portfolio</a>
                            </li>
                            <li>
                                <a class="page-scroll" href="#contactt">Contact</a>
                            </li>
                            <li>
                                <a class="page-scroll" asp-page="./Index">PL</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </nav>

        <div id="myCarousel" class="carousel slide carousel-fade" data-ride="carousel">
            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
            </ol>
            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                <div class="item carusel1 active"></div>
                <div class="item carusel2"></div>
                <div class="item carusel3"></div>
                <div class="item carusel4"></div>
            </div>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="footer-block visible-over" id="contactt">
        <div class="container">
