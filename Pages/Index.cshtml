    <!-- Contact Section -->
    <section id="contact">
        <div class="bg-filter"></div>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <h4 class="section-name fade-animate">Kontakt<i class="fa fa-minus" aria-hidden="true"></i></h4>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-10 col-sm-offset-1 col-md-6 col-md-offset-3">
                    <h1 class="contact-heading section-heading fade-animate delay4">say hello</h1>

                    @using (Html.BeginForm(FormMethod.Post, new { id = "contactForm", novalidate=true }))
                    {
                        <div class="row">
                            <div class="form-group col-md-12">
                                <input asp-for="Name" type="text" class="form-control" placeholder="Imię *" id="name" required data-validation-required-message="Prosimy o podanie imienia.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group col-md-12">
                                <input asp-for="Email" type="email" class="form-control" placeholder="Email *" id="email" required data-validation-required-message="Prosimy o podanie adresu email.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group col-md-12">
                                <input asp-for="Subject" type="text" class="form-control" placeholder="Tytuł *" id="subject" required data-validation-required-message="Prosimy wpisać tytuł.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="clearfix"></div>
                            <div class="form-group col-md-12">
                                <textarea asp-for="Message" class="form-control" placeholder="Wiadomość *" id="message" required data-validation-required-message="Prosimy wpisać wiadomość."></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-md-12 text-center fade-animate">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Wyślij wiadomość</button>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>


